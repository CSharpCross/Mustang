{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "types": ["reflect-metadata"],
    "sourceMap": true,
    "outDir": "build",
    "rootDir": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "gulpfile.ts",
    "src/static"
  ]
}